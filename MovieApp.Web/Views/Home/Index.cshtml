@model Movie

@section header
{
    @await Html.PartialAsync("_header")
}

@section scripts
{
    <script src="~/js/scripts.js"></script>
}



    <h1>Film Lİstesi</h1>
    <div id="filmler">
        <div class="film">
            <p>@Model.Title</p>
            <p>@Model.Description</p>
            <p>@Model.Director</p>
            <p>
                <ul>
                    @foreach (var player in @Model.Players)
                    {
                        <li>@player</li>
                    }
                </ul>
            </p>
        </div>
    </div>
